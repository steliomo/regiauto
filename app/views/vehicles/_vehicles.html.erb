<table>
	<tr>
		<th>Number plate</th>
		<th>Mark</th>
		<th>Model</th>
		<th>Engine Number</th>
		<th>Cylinders</th>
		<th>Cylinders Size</th>	
	</tr>
	<% @vehicles.each do |vehicle| %>
		<tr>
			<td onclick="location.href='<%=vehicle_path(vehicle)%>'"><%= vehicle.name %></td>
			<td><%= vehicle.mark %></td>
			<td><%= vehicle.model %></td>
			<td><%= vehicle.engine_number %></td>
			<td><%= vehicle.cylinders %></td>
			<td><%= vehicle.cylinders_cc %></td>
		</tr>
	<% end %>
</table>
